# eToro API Configuration
# Copy this file to .env.local and fill in your actual API credentials

# eToro API Base URL
ETORO_API_BASE_URL=https://www.etoro.com/api/public

# eToro API Authentication Keys
# Get these from eToro's developer portal
ETORO_API_KEY=your_api_key_here
ETORO_USER_KEY=your_user_key_here

# Alternative environment variable names (for backwards compatibility)
DEFAULT_BASE_URL=https://www.etoro.com/api/public
DEFAULT_API_KEY=your_api_key_here
DEFAULT_USER_KEY=your_user_key_here